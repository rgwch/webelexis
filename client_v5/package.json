{
  "name": "Webelexis",
  "description": "A Svelte-powered Elexis client",
  "version": "3.5.4",
  "scripts": {
    "build": "rollup -c .config/rollup.config.js",
    "clean": "rimraf public/build",
    "dev": "rollup -c .config/rollup.config.js -w",
    "start": "sirv public -s --port 3000",
    "validate": "svelte-check",
    "storybook": "start-storybook -p 6006 -c .config/storybook",
    "xstorybook": "export NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006 -c .config/storybook",
    "build-storybook": "build-storybook -c .config/storybook",
    "test": "jest -c .config/jest.config.js",
    "test:watch": "npm test -- --watch",
    "i18n": "sync-i18n --files '**/i18n/*.json' --primary de --languages en fr it --space 2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@feathersjs/authentication-client": "^4.5.13",
    "@feathersjs/client": "^4.5.13",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.0.0",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.4.18",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^13.5.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/feathersjs__authentication-client": "^1.0.9",
    "@types/feathersjs__socketio-client": "^1.0.7",
    "@types/jest": "^27.4.0",
    "@types/luxon": "^2.0.9",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.2",
    "i18next-json-sync": "^2.3.1",
    "jest": "^27.4.7",
    "jest-svelte-events": "^1.0.5",
    "lru-cache": "^7.10.1",
    "luxon": "^2.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^2.0.1",
    "socket.io-client": "^2.4.0",
    "svelte": "^3.46.2",
    "svelte-check": "^2.3.0",
    "svelte-fa": "^2.4.0",
    "svelte-i18n": "^3.3.13",
    "svelte-jester": "^2.1.5",
    "svelte-loader": "^3.1.2",
    "svelte-navigator": "^3.1.5",
    "svelte-preprocess": "^4.7.4",
    "svelte-windicss-preprocess": "^3.5.0",
    "sync-i18n": "0.0.18",
    "ts-jest": "^27.1.3",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "uuid": "^8.3.2",
    "windicss-webpack-plugin": "^1.6.2"
  },
  "dependencies": {
    "@feathersjs/feathers": "^4.5.12",
    "@feathersjs/socketio-client": "^4.5.13",
    "@tiptap/core": "^2.0.0-beta.175",
    "@tiptap/starter-kit": "^2.0.0-beta.184",
    "@types/lru-cache": "^7.10.10",
    "svelte-infinite-scroll": "^2.0.1",
    "svelte-tabs": "^1.1.0"
  }
}
